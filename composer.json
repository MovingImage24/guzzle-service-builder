{
  "name": "mi/guzzle-service-builder",
  "description": "Build guzzle command clients from files",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Alexander Miehe",
      "email": "alexander.miehe@movingimage.com"
    }
  ],
  "require": {
    "guzzlehttp/guzzle": "~5.0",
    "guzzlehttp/guzzle-services": "^0.5.0",
    "webmozart/json": "^1.0",
    "jms/serializer": "^1.0",
    "puli/repository": "^1.0@beta"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.7",
    "symfony/filesystem": "~2.7.0"
  },
  "autoload": {
    "psr-4": {
      "Mi\\Guzzle\\ServiceBuilder\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mi\\Guzzle\\ServiceBuilder\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "minimum-stability": "beta"
}
